    Private Function GetDataTable(sql As String) As DataTable
        'Send SQL query to database...

        Dim dt As New DataTable("sp500")

        dt.Columns.Add("company")
        dt.Columns.Add("symbol")

        For Each record In _mockData
            Dim dr As DataRow = dt.NewRow

            dr("company") = record.Company
            dr("symbol") = record.Symbol

            dt.Rows.Add(dr)
        Next

        Return dt
    End Function

    Private Sub LoadComboWithBindingList()
        '------------------------------------------------------------------------
        CmbComboBindingList.Columns.Clear()
        CmbComboBindingList.Columns.Add("Company", 80)
        CmbComboBindingList.Columns.Add("Symbol", 20)
        '------------------------------------------------------------------------

        CmbComboBindingList.DisplayMember = "Company"
        CmbComboBindingList.ValueMember = "Symbol"
        CmbComboBindingList.DataSource = _mockData
    End Sub

    Private Sub LoadComboWithBindingDictionary()
        '------------------------------------------------------------------------
        CmbComboBindingDictionary.Columns.Clear()
        CmbComboBindingDictionary.Columns.Add("Company", 80)
        CmbComboBindingDictionary.Columns.Add("ID", 20)
        '------------------------------------------------------------------------

        Dim dic As New Dictionary(Of ULong, Object)

        For Each record In _mockData
            dic.Add(record.Id, record.Company)
        Next

        CmbComboBindingDictionary.DisplayMember = "Value"
        CmbComboBindingDictionary.ValueMember = "Key"
        CmbComboBindingDictionary.DataSource = dic.ToList()
        'It works with BindingSource as well, keeping compatibility
        'CmbComboBindingDictionary.DataSource = New BindingSource(dic, Nothing)
    End Sub

    Private Sub LoadComboWithBindingDataTable()
        '------------------------------------------------------------------------
        CmbComboBindingDataTable.Columns.Clear()
        CmbComboBindingDataTable.Columns.Add("Company", 80)
        CmbComboBindingDataTable.Columns.Add("Symbol", 20)
        '------------------------------------------------------------------------

        Dim sql As String = "SELECT company, symbol FROM sp500"
        Dim dt As DataTable = GetDataTable(sql)

        'It's also possible to load many columns from the DataTable and choose one as key with ValueMember

        CmbComboBindingDataTable.DisplayMember = "company"
        CmbComboBindingDataTable.ValueMember = "symbol"
        CmbComboBindingDataTable.DataSource = dt
    End Sub

    Private Sub LoadComboWithListViewItems()
        '------------------------------------------------------------------------
        CmbComboListViewItems.Columns.Clear()
        CmbComboListViewItems.Columns.Add("Company", 50)
        CmbComboListViewItems.Columns.Add("Sector", 30)
        CmbComboListViewItems.Columns.Add("Symbol", 20)
        '------------------------------------------------------------------------

        CmbComboListViewItems.Items.Clear()

        For Each record In _mockData
            Dim it As New ListViewItem

            it.Text = record.Company
            it.SubItems.Add(record.Sector)
            it.SubItems.Add(record.Symbol)

            CmbComboListViewItems.Items.Add(it)
        Next
    End Sub

    Private Sub LoadComboWithGroups()
        '------------------------------------------------------------------------
        CmbComboGroups.Columns.Clear()
        CmbComboGroups.Columns.Add("Company", 80)
        CmbComboGroups.Columns.Add("Symbol", 20)
        '------------------------------------------------------------------------

        CmbComboGroups.Items.Clear()

        For Each record In _mockData
            Dim it As New ListViewItem

            it.Text = record.Company
            it.SubItems.Add(record.Symbol)

            If CmbComboGroups.Groups(record.Sector) Is Nothing Then
                it.Group = CmbComboGroups.Groups.Add(record.Sector, record.Sector)
            Else
                it.Group = CmbComboGroups.Groups(record.Sector)
            End If

            CmbComboGroups.Items.Add(it)
        Next
    End Sub

    Private Sub CmbComboListViewItems_SelectionChangeCommitted(sender As System.Object, e As System.EventArgs) Handles CmbComboBindingList.SelectionChangeCommitted, CmbComboBindingDictionary.SelectionChangeCommitted,
                                                                                                                        CmbComboBindingDataTable.SelectionChangeCommitted, CmbComboListViewItems.SelectionChangeCommitted,
                                                                                                                        CmbComboGroups.SelectionChangeCommitted
        Dim cmb = CType(sender, MultiComboBox.MultiCombo)

        TxtEvents.Clear()

        TxtEvents.AppendText("Name: " & cmb.Name & vbNewLine)
        TxtEvents.AppendText("Items.Count: " & cmb.Items.Count & vbNewLine)
        TxtEvents.AppendText("CheckedItems.Count: " & cmb.CheckedItems.Count & vbNewLine)
        TxtEvents.AppendText("SelectedIndex: " & cmb.SelectedIndex & vbNewLine)
        TxtEvents.AppendText("SelectedValue: " & cmb.SelectedValue)

        If cmb.Text.Length > 100 Then
            TxtEvents.AppendText("Text: " & Mid(cmb.Text, 1, 100) & "..." & vbNewLine)
        Else
            TxtEvents.AppendText("Text: " & cmb.Text & vbNewLine)
        End If
    End Sub
